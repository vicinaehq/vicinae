{
	"$import": [
		"keybinds.json"
	],
    "closeOnFocusLoss": false,
    "considerPreedit": true,
    "faviconService": "google",
    "font": {
        "normal": "Departure Mono",
        "size": 10.5
    },
	"providers": {
		"shortcuts": {
		},
		"apps": {
		},
		"vicinae.store.nix": {
			"preferences": {
				"token": "HELLO"
			},
			"entrypoints": {
				"pull-requests": {
					"enabled": true,
					"preferences": {
						"key": 1
					}
				}
			}
		}
	},
    "keybinding": "default",
    "keybinds": {
        "action.copy": "control+shift+C",
        "action.copy-name": "control+shift+.",
        "action.copy-path": "control+shift+,",
        "action.dangerous-remove": "control+shift+X",
        "action.duplicate": "control+D",
        "action.edit": "control+E",
        "action.edit-secondary": "control+shift+E",
        "action.move-down": "control+shift+O",
        "action.move-up": "control+shift+,",
        "action.new": "control+N",
        "action.open": "control+O",
        "action.pin": "control+shift+P",
        "action.refresh": "control+R",
        "action.remove": "control+X",
        "action.save": "control+S",
        "open-search-filter": "control+P",
        "open-settings": "control+,",
        "toggle-action-panel": "control+SPACE"
    },
    "popToRootOnClose": false,
    "rootSearch": {
        "searchFiles": true
    },
    "theme": {
        "iconTheme": "Papirus-Dark",
        "name": "vicinae-dark"
    },
    "window": {
        "csd": false,
        "opacity": 0.95,
        "rounding": 10
    }
}
