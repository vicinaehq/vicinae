{
  "manifest_version": 3,
  "name": "Vicinae Integration",
  "version": "1.0.0",
  "description": "Search and manage your browser tabs directly from your vicinae launcher",
  "homepage_url": "https://vicinae.com",
  "permissions": [
    "nativeMessaging",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Vicinae Integration",
    "default_icon": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png"
	}
  }
}
